extends base

block content 
    form(method="POST")
        input(placeholder="사용자의 이름을 입력하세요",name="name",type="text",required)
        input(placeholder="이메일을 입력하세요",name="email",type="email",required)
        input(placeholder="아이디를 입력하세요",name="username",type="text",required)
        input(placeholder="비밀번호를 입력하세요",name="password",type="password",required)
        input(placeholder="주소를 입력하세요",name="location",type="text",required)
        input(type="submit", value="회원가입")
